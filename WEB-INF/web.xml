<?xml version="1.0" encoding="utf-8"?>
<web-app id="iocaste_tests" xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5"> 
    <description>iocaste login</description>
    <display-name>iocaste login</display-name>
    
    <servlet>
        <servlet-name>iocaste_login</servlet-name>
        <servlet-class>org.iocaste.login.LoginServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>iocaste_login</servlet-name>
        <url-pattern>/login.jsp</url-pattern>
    </servlet-mapping>
</web-app>